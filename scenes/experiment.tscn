[sub_resource type="GDScript" id=2]
script/source = "extends Path

onready var pointNode:= $Point as Spatial; 

func _ready() -> void:
	assert(pointNode)
	remove_child(pointNode)
	connect(\"curve_changed\",self, \"_curve_changed\")
	_curve_changed()

func _curve_changed() -> void:
	if !curve:
		return
	for child in get_children():
		(child as Node).queue_free()
	for p in curve.tessellate():
		var node := pointNode.duplicate(DUPLICATE_USE_INSTANCING) as Spatial
		node.global_transform.origin = p
		add_child(node)
		
"

