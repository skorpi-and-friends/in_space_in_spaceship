[sub_resource type="GDScript" id=13]
script/source = "extends Node

export var input := Vector2(90, 0);
export var output := 91;

func _ready() -> void:
	var ref := funcref(self, \"_generate_basis\");
	ref.call_func();
	_is_class();
	_smoothstep();
	_csharp_static_functions();
	_csharp_enums();
	_AABB_support_point();

func _generate_basis() -> void:
	print(\"we're here\");
	var forward := Vector3.FORWARD;
	var up := Vector3.UP;
	
	var a := Transform().looking_at(forward, up).basis;
	var z := -forward.normalized();
	var x := up.cross(z).normalized();
	var y := z.cross(x).normalized();
	
	var b :=  Basis(
			Vector3(x.x, y.x, z.x),
			Vector3(x.y, y.y, z.y),
			Vector3(x.z, y.z, z.z)
		);
	assert(a == b);


func _process(delta: float) -> void:
	output = Utility.delta_angle_deg(input.x, input.y);

func _is_class() -> void:
	var craft_master = CraftMaster.new();
	print(\"is craft master %s\" % craft_master.is_class(\"CraftMaster\"));

func _smoothstep() -> void:
	print(\"smoothstep experiment\");
	print(\"from: %s, to: %s, weight: %s -> %s\" % [0.0, 2.0, 0.0, smoothstep(0, 2, 0)]);
	print(\"from: %s, to: %s, weight: %s -> %s\" % [0.0, 2.0, 0.5, smoothstep(0, 2, 0.5)]);
	print(\"from: %s, to: %s, weight: %s -> %s\" % [0.0, 2.0, 1.0, smoothstep(0, 2, 1)]);
	print(\"from: %s, to: %s, weight: %s -> %s\" % [0.0, 2.0, 1.5, smoothstep(0, 2, 1.5)]);
	print(\"from: %s, to: %s, weight: %s -> %s\" % [0.0, 2.0, 2.0, smoothstep(0, 2, 2)]);
	print(\"from: %s, to: %s, weight: %s -> %s\" % [0.0, 2.0, 3.0, smoothstep(0, 2, 3)]);
	print(\"from: %s, to: %s, weight: %s -> %s\" % [0.0, 2.0, 4.0, smoothstep(0, 2, 3)]);
	print(\"from: %s, to: %s, weight: %s -> %s\" % [0.0, 2.0, 8.0, smoothstep(0, 2, 8)]);

func _csharp_static_functions() ->void:
	var sharpscript: CSharpScript= preload(\"res://scripts/utility/ExperimentTestBed.cs\");
	sharpscript.SomeStaticFunc();


func _csharp_enums() ->void:
	var sharpscript: CSharpScript= preload(\"res://scripts/ai_csharp/Relationship.cs\");
#	print(sharpscript.Neutral);
#	print(sharpscript.Hostile);



func _AABB_support_point() -> void:
	var aabb = AABB(Vector3(-1, -1, -1), Vector3(2, 2, 2));
	print(\"aabb support point(12, 32, 5) -> %s\" % aabb.get_support(
			Vector3(12, 32, 5).normalized()));
"

